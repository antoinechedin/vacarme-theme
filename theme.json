{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "useRootPaddingAwareAlignments": false,
        "layout": {
            "contentSize": "1000px",
            "wideSize": "1000px"
        },
        "typography": {
            "fontFamilies": [
                {
                    "name": "Primary",
                    "slug": "primary",
                    "fontFamily": "'Avara Bold'",
                    "fontFace": [
                        {
                            "fontFamily": "'Avara Bold'",
                            "fontWeight": "normal",
                            "fontStyle": "normal",
                            "src": [
                                "file:./fonts/Avara-Bold.woff2"
                            ]
                        },
                        {
                            "fontFamily": "'Avara Bold'",
                            "fontWeight": "normal",
                            "fontStyle": "italic",
                            "src": [
                                "file:./fonts/Avara-BoldItalic.woff2"
                            ]
                        },
                        {
                            "fontFamily": "'Avara Bold'",
                            "fontWeight": "bold",
                            "fontStyle": "normal",
                            "src": [
                                "file:./fonts/Avara-Black.woff2"
                            ]
                        }
                    ]
                }
            ]
        }
    },
    "styles": {},
    "customTemplates": [],
    "templateParts": [
        {
            "area": "header",
            "name": "header",
            "title": "Header"
        }
    ],
    "patterns": []
}